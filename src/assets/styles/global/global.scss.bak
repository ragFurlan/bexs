body {
  background-color: #fff;
  color: #444;
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
}

.header {
  padding: 15px 0;
  background-color: #fff;
  box-shadow: 0 0px 8px 3px rgba(0,0,0,0.1);
  position: relative;
  top: 0;
}

h2 {
  color: #4a4a4a;
  font-size: 1.8em;
}

h3 {
  color: #4a4a4a;
  font-size: 1.5em;
}

ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

p {
  font-size: 1.2em;
  color: #4a4a4a;
  opacity: .9;
}

p span {
  color: #ed0f69;
  font-weight: 600;
}

a {
  color: #ed0f69;
}

a:hover {
  color: #ed0f69;
  text-decoration: underline;
}

small {
  font-size: .9em;
  font-weight: 300;
}

.none {
  display: none !important;
}

.no-padding {
  padding: 0 !important;
}

.no-margin {
  margin: 0 !important;
}

.right {
  float: right !important;
}

.left {
  float: left !important;
}

ul.paginacao {
  margin-top: 10px;
}

.paginacao li {
  display: inline-block;
  padding: 0 10px 0 0;
}

.paginacao h4 {
  font-size: 1.5em;
  position: relative;
  top: -4px;
}

.paginacao i {
  color: #ed0f69;
  font-size: 2.2em;
  margin-top: 10px;
}

/******* Cabeçalho ********/
.header img {
  height: 40px;
}

.header h2 {
  margin: 3px 0 0px 0;
  text-align: center;
}

.header .dropdown {
  margin-top: 5px;
}

.header .usuario-logado {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}


.dropdown a.menu {
  font-size: 1.2em;
  color: #ed0f69;
}

.header .dropdown #local-exame {
  font-size: 1.2em;
  color: #4a4a4a;
}

.dropdown a.menu:hover {
  color: #ed0f69;
  text-decoration: none;
}

.dropdown a.menu:focus {
  text-decoration: none;
}

.dropdown a.menu i {
  font-size: 1.4em;
  color: #ed0f69;
}

.open > .dropdown-menu {
  border-radius: 0;
  margin-top: 25px;
  width: 100%;
  border-top: 3px solid #ed0f69;
  padding: 0;
}

.dropdown-menu li a {
  font-size: 1.2em;
  opacity: .7;
  padding: 7px 20px;
}

.dropdown-menu a:hover {
  background-color: transparent;
  color: #000;
  opacity: 1;
}

.dropdown-menu i {
  color: #fecd1f;
}

.dropdown-menu li {
  padding: 0;
  margin: 0;
}

.header .dropdown-menu li.logout {
  background-color: #f7f7f7;
}

.header .btn-atualizar i {
  margin-top: 4px;
  font-size: 2.3em;
  color: #ed0f69;
}


.titulo-cabecalho {
  padding: 15px 0;
  border-bottom: 1px solid #ddd;
  background-color: #fbfbfb;
  margin-bottom: 10px;
}

.titulo-cabecalho h2 {
  font-weight: 600;
  font-size: 1.5em;
  text-align: center;
  margin-bottom: 20px;
  text-transform: uppercase;
}

.titulo-cabecalho h2 span {
  text-align: center;
  margin-bottom: 20px;
  font-size: .8em;
  text-transform: uppercase;
}


/*** Agendamento ***/

.agendamento {
  padding: 25px 0;
}

.dados-paciente {
  margin-bottom: 60px;
}

.dados-paciente h3, .dados-realizacao h3, .etapas h3 {
  margin-bottom: 40px;
}

.dados-paciente #rneCampo {
  display: none;
}

.etapas-atendimento h3 {
  margin-left: 15px;
}

.etapas-atendimento .btn {
  width: 100% !important;
}

.exames-agendamento {
  margin: 30px 0;
}

.exames-agendamento ul li {
  width: 130px;
  text-align: center;
  float: left;
  margin-bottom: 30px;
}

.exames-agendamento .exame-icon {
  width: 100px;
  height: 100px;
  border-radius: 100px;
  display: inline-block;
  background-color: #fff;
  text-align: center;
  border: 5px solid #ccc;
  margin-bottom: 30px;
}

.exames-agendamento .btn-exame-selecao {
  display: block;
  width: 35px;
  height: 34px;
  border: 1.5px solid;
  border-radius: 35px;
  margin: 0 auto;
  position: relative;
  background-color: #fff;
  cursor: pointer;
  border-color: #ccc;
}

.exames-agendamento .btn-exame-selecao:hover {
  text-decoration: none;
}

.exames-agendamento .btn-exame-selecao i {
  color: green;
  font-size: 1.5em;
  margin-top: 3px;
  display: none;
  line-height: 30px;
}

.exames-agendamento .exame-nome {
  display: block;
  text-align: center;
  text-transform: uppercase;
  height: 50px;
  float: left;
  width: 100%;
}

.exames-agendamento .selecionado, .exames-agendamento .selecionado .btn-exame-selecao {
  border-color: #ed0f69;
}

/*** Ponto de Contato ***/

.ponto-contato {
}

.ponto-contato .container {
  width: 100%;
}

.lista-pacientes li {
  background-color: #e9e9e9;
  display: inline-block;
  width: 40%;
  margin-right: 5px;
}

.lista-pacientes i.paciente {
  color: #ed0f69;
  font-size: 4em;
  margin: 40px 0 0 0;
}

.lista-pacientes span {
  color: #ed0f69;
}

.lista-pacientes .img-paciente {
  border-radius: 50px;
  background-color: #444;
  height: 60px;
  width: 60px;
  display: block;
}

.paciente-atendimento {
  padding: 10px 25px;
  width: 73%;
  height: 182px;
}

.lista-pacientes .info-paciente {
  display: block;
  width: 75%;
  margin: 10px 0;
}

.status-atendimento {
  background-color: #f2f2f2;
  text-align: center;
  width: 27%;
  height: 182px;
  display: table-cell;
  vertical-align: middle;
}

.status-atendimento .btn-popover {
  position: relative;
  font-size: 1em;
}


.status-atendimento i {
  font-size: 3em;
  line-height: 0;
}

.status-atendimento i.ion-ios-clock {
  color: #ed0f69;
}

.status-atendimento i.ion-checkmark-circled {
  color: green;
  font-size: 2.8em;
}

ul.avaliacao-atendimento {
  margin-top: 10px;
}

.avaliacao-atendimento li {
  display: inline-block;
  font-size: .35em;
  width: 3px;
  background-color: transparent;
  color: #ccc;
}

.avaliacao-atendimento i.ion-ios-chatbubble {
  font-size: 5em;
  margin: 0 0 0 7px;
  color: #ed0f69;
}

.avaliacao-atendimento i.active {
  color: #ed0f69;
}

.media-dia {
  width: 100%;
  text-align: center;
  vertical-align: middle;
}

.media-dia h3 {
  font-size: 2em;
}

.media-dia p {
  font-size: 4em;
}

.pontuacao-media li {
  display: inline-block;
  font-size: 3em;
  background-color: transparent;
  color: #ccc;
}

.pontuacao-media i.active {
  color: #ed0f69;
}




/**** Modal ****/
.modal-icon {
  border: 3px solid #ed0f69;
  height: 70px;
  width: 70px;
  border-radius: 50px;
}

.btn-modal {
  border: 3px solid #ed0f69;
  border-radius: 90px;
  width: 90px;
  height: 90px;
}

/**** Avaliação Modal ****/
.estrelas {
  text-align: center;
}

.estrelas input[type=radio] {
  display: none;
}

.estrelas label i.fa:before {
  content: '\f005';
  color: #ed0f69;
  font-size: 3em;
}

.estrelas input[type=radio]:checked ~ label i.fa:before {
  color: #CCC;
}

/************ Grade Agendamento ******************/
.btn-popover {
  display: block;
  width: 100%;
  font-size: 3em;
  color: #ccc;
  line-height: 0;
  cursor: pointer;
  position: absolute;
  bottom: 0;
}

.btn-popover:focus {
  color: #ed0f69;
  outline: none;
}

.popover-content {
  text-align: center;
}

.popover-content i.ion-checkmark-circled {
  color: green;
  font-size: 3em;
  margin: 0 !important;
  line-height: 0;
  cursor: pointer;
}

.popover-content i.ion-chatbubble, .popover-content i.ion-ios-bell {
  color: #CC0738;
  font-size: 3.2em;
  margin: 0 !important;
  line-height: 0;
  cursor: pointer;
}

.grade-paciente {
  width: 100%
}

.exame-status {
  width: 75px;
  display: table-cell;
  text-align: center;
  vertical-align: middle;
  height: 160px;
  padding: 10px 0 0 0;
}

.exame-status i.ion-checkmark-circled {
  color: green;
  font-size: 3em;
  margin: 0 !important;
}

.exame-status i.ion-ios-clock {
  color: #ed0f69;
  font-size: 3.2em;
  margin: 0 !important;
}

.exame-status i.ion-close-circled {
  color: red;
  font-size: 3em;
  line-height: 3.2;
}

.exame-status .avaliacao-atendimento li {
  width: 7px;
}

.exame-status .avaliacao-atendimento i.ion-android-star {
  font-size: 3em;
  width: 10px;
}

td.desabilitado {
  -webkit-box-shadow: 0 0px 40px 3px #ccc inset;
  -moz-box-shadow: 0 0px 40px 3px #ccc inset;
  box-shadow: 0 0px 40px 3px #ccc inset;
}


#fixarcoluna table {
  border: none;
}

#fixarColuna .content table {
  width: 100%;
}
/*#fixarColuna .content tr {background-color: #e9e9e9;}*/
/* #fixarColuna .content tr:hover {background-color: #949fab;} */
#fixarColuna .content td {
  border-bottom: solid 1px #ddd;
  border-right: solid 1px #ddd;
  padding: 5px 20px;
  width: 80px;
  height: 170px;
}

#fixarColuna .content td i.ion-checkmark-circled {
  color: green;
  font-size: 3em;
  margin-top: 40px;
}

#fixarColuna .content td i.ion-android-alert {
  color: #FFC401;
  font-size: 3.3em;
  margin-top: 40px;
}

#fixarColuna .contentHead td {
  border-right: solid 1.5px #DDD;
  padding: 5px;
  background-color: #fff;
}

#fixarColuna .contentHead tr {
  background-color: #F0F1F2;
}

#fixarColuna .content {
  display: block;
  width: 100%;
  padding: 0;
  margin: 0;
  overflow-y: hidden;
  height: 495px;
  background: #fafafa
}

#fixarColuna .contentHead {
  display: block;
  width: 98.5%;
  padding: 0;
  margin: 0;
  overflow: hidden;
  height: 85px;
}

#fixarColuna .contentHead img {
  display: block;
  position: relative;
  margin: 0 auto;
  width: 105px;
  padding: 0px 15px;
}

#fixarColuna .content div {
  display: inline;
  float: left;
  position: relative;
  border: none;
}

#fixarColuna .contentHead div {
  display: inline;
  float: left;
  position: relative;
  border: none;
  width: 100%;
}


#fixarColuna .contentHead .headerPaciente {
  width: 20%;
}

#fixarColuna .contentHead #colHead {
  width: 80%;
  overflow-x: hidden;
}

#fixarColuna .content #colPacientes {
  width: 19.7%;
  overflow-y: hidden;
  overflow-x: scroll;
  height: 495px;
}

#fixarColuna .content #fixBody {
  width: 80%;
  overflow: scroll;
  height: 495px;
  z-index: 2;
}

#ocultarScrollPacientes {
  position: absolute;
  background-color: #f2f2f2;
  width: 100%;
  height: 17px;
  z-index: 1;
  margin-top: 558px;
}

@media screen and (max-width: 768px) {
  .header img {
    height: 30px !important;
  }

  .header .dropdown, .header h4 {
    text-align: left !important;
  }
}


@media screen and (max-width: 1200px) {
  .titulo-cabecalho h2 {
    text-align: left !important;
  }

  .lista-pacientes li {
    width: 48.95% !important;
  }

  .info-paciente p {
    font-size: 1em !important;
  }

  .avaliacao-atendimento li {
    width: 3px !important;
  }

  .avaliacao-atendimento i.ion-ios-chatbubble {
    margin: 0 0 0 2px !important;
  }

  .exame-status .avaliacao-atendimento li {
    width: 7px !important;
  }


  #fixarColuna .contentHead #colHead {
    width: 67.6% !important;
  }

  #fixarColuna .contentHead .headerPaciente {
    width: 32.4% !important;
  }

  #fixarColuna .content #colPacientes {
    width: 31.9% !important;
  }

  #fixarColuna .content #fixBody {
    width: 68% !important;
  }

  #ocultarScrollPacientes {
    margin-top: 578px !important;
  }
}

@media screen and (max-width: 533px) {
  .lista-pacientes li {
    width: 100% !important;
  }

  .avaliacao-atendimento li {
    width: 3% !important;
  }
}

@media screen and (max-width: 320px) {
  .checkup-etapas ul > li {
    width: 32.2% !important;
    height: 90px !important;
    margin: 4.5% .2% !important;
    border-radius: 100px !important;
  }
}
